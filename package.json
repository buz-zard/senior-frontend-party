{
  "name": "testio",
  "version": "0.0.1",
  "description": "testio server explorer",
  "author": "Karolis Å arapnickis <pastas.k@gmail.com>",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --port $npm_package_config_port",
    "build": "rimraf build && cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "prepush": "npm run qa",
    "qa": "run-p test lint",
    "test": "jest",
    "lint": "eslint src",
    "tdd": "npm run test -- --watch",
    "test-build": "npm run build && serve -s build"
  },
  "config": {
    "port": 3000
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "history": "^4.7.2",
    "i18next": "^9.0.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-redux": "next",
    "recompose": "^0.25.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.0.3",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.0.2",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^20.0.4",
    "npm-run-all": "^4.1.1",
    "path": "^0.12.7",
    "rimraf": "^2.6.1",
    "serve": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "keywords": [
    "react"
  ],
  "license": "MIT"
}
